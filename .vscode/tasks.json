{
    "tasks": [
        {
            "type": "shell",
            "label": "gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-ggdb3",
                "-I",
                "${fileDirname}/../lib",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "${fileDirname}/../libunp.a",
                "-lpthread",
            ],
            "options": {
                "cwd": "/usr/bin"
            }
        }
    ],
    "version": "2.0.0"
}